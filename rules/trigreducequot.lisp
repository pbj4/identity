; inverse quotient identities
(=> (* (sin u) (/ (cos u))) (tan u))
(=> (* (cos u) (/ (sin u))) (cot u))

(=> (* n (sin u) (/ (cos u))) (* n (tan u)))
(=> (* n (cos u) (/ (sin u))) (* n (cot u)))

(=> (* n (sin u) (/ (* d (cos u)))) (* n (/ d) (tan u)))
(=> (* n (cos u) (/ (* d (sin u)))) (* n (/ d) (cot u)))
